{
  "rewrites": [
    {
      "source": "/api/test/voice-order",
      "destination": "/api/test-voice-order"
    },
    {
      "source": "/webhook/voice-order/:path*",
      "destination": "/api/webhook-voice-order"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install"
}
